{
  "name": "rating-ui-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.14.0 <23.0.0",
    "npm": ">=10.9.2 <11.0.0"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "npm run type-check && npm run lint:js && npm run lint:scss && npm run format && npm run build-only",
    "type-check": "tsc --build",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix strict",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "format": "prettier --check src/",
    "format:fix": "prettier --write src/",
    "build-only": "vite build",
    "preview": "vite preview",
    "commit": "commit"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/prompt-cli": "19.8.1",
    "@eslint/js": "9.30.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "sass-embedded": "1.90.0",
    "stylelint": "16.23.1",
    "stylelint-config-recess-order": "7.2.0",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-order": "7.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vite": "7.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.{ts,tsx})": "prettier --write --ignore-unknown"
  }
}
